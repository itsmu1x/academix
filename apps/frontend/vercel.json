{
	"version": 2,
	"buildCommand": "npm run build",
	"outputDirectory": "build/client",
	"installCommand": "npm install",
	"framework": "vite",
	"builds": [
		{
			"src": "build/server/index.js",
			"use": "@vercel/node"
		}
	],
	"routes": [
		{
			"src": "/build/client/(.*)",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"dest": "/build/client/$1"
		},
		{
			"src": "/(.*)",
			"dest": "build/server/index.js"
		}
	]
}
